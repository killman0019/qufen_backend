<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
            
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li>
                <a href="${www}/mediareports">媒体报道列表</a>
            </li>
            <li class="active">修改媒体报道</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>

    <div class="page-content">
        <div class="page-header">
            <h1>修改媒体报道</h1>
        </div>
        
        #if(${msg})
        	<div>
			#if(${success})
        		<li class="text-success">$!{msg}</li>
   			 #else
        		<li class="text-danger">$!{msg}</li>
   			 #end    	
   			</div>
   		#end        
        
        <form  class="form-horizontal">
             <div class="row">
	                <div class="col-xs-12 col-md-8">
	                    <div class="form-group">
	                        <label class="col-md-4 col-sm-2 control-label">媒体logo地址：</label>
	                        <div class="col-md-8 col-sm-8">
	                        	$!{mediareports.vcLogoUrl}
	                        </div>
	                    </div>
	                </div>
	          </div>
	          <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">已上传的图片：</label>
                        <div class="col-md-8 col-sm-8">
                        	<img src="$!{staticUploadUrl}${mediareports.vcLogoUrl}"  alt="图标" />
                        </div>
                    </div>
                </div>
              </div>
              <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">媒体链接地址：</label>
                        <div class="col-md-8 col-sm-8">
                        	<a href="$!{mediareports.vcUrl}">$!{mediareports.vcUrl}</a>
                        </div>
                    </div>
                </div>
             </div>
             <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">顺序：</label>
                        <div class="col-md-8 col-sm-8">
                        	$!{mediareports.isort}
                        </div>
                    </div>
                </div>
             </div>
             <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">状态：</label>
                        <div class="col-md-8 col-sm-8">
                            #if($mediareports.istate ==1) 
                            	有效
                        	#elseif($mediareports.istate ==2) 
                        		无效
                            #end
                        </div>
                    </div>
                </div>
             </div>  
            <div class="clearfix form-actions">
                <div class="row">
                    <div class="col-md-8">
                        <div class="col-sm-offset-4 col-md-9">
                            <button class="btn" type="button" onclick="javascript:history.go(-1);">
                                <i class="icon-arrow-left bigger-110"></i>返回
                            </button>
                        </div>
                    </div>
                </div>
            </div>
     </div>
    </form>
</div><!-- /.page-content -->
</div><!-- /.main-content -->
