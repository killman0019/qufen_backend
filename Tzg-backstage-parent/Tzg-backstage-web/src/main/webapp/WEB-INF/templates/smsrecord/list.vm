<script src="${www}/js/my97DatePicker/WdatePicker.js"></script>
<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li class="active">短信列表</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>
	<div class="page-content">
		<div class="page-header">
		    <h1>短信列表</h1>
		</div>
		<div class="container-fluid">
		    <div class="dataTables_wrapper">
		        <div class="row">
		            <div class="col-sm-11">
		                <form class="form-inline" role="form">
							短信接口:
							 <select name="sinterface">
	                        		<option value="">全部</option>
		                            <option value="1" #if($query.sinterface ==1) selected #end>漫道</option>
		                            <option value="2" #if($query.sinterface ==2) selected #end>建周</option>
								    <option value="3" #if($query.sinterface ==3) selected #end>亿美</option>
								    <option value="4" #if($query.sinterface ==4) selected #end>示远</option>
		                        </select>
							发送状态:
								 <select name="istate">
	                        		<option value="">全部</option>
		                            <option value="1" #if($query.istate ==1) selected #end>成功</option>
		                            <option value="2" #if($query.istate ==2) selected #end>失败</option>
		                        </select>
							<div class="form-group">
		                        <label>发送时间：</label>
		                        <input type="text" name="dtSendBegin" value="$!{query.dtSendBegin}" id="dtSendBegin" onFocus="WdatePicker({lang:'zh-cn'})" value="$!{query.dtCountBegin}" class="input-date">
								-
								<input type="text" name="dtSendEnd" value="$!{query.dtSendEnd}"  id="dtSendEnd" onFocus="WdatePicker({lang:'zh-cn'})" value="$!{query.dtCountEnd}" class="input-date">
		                    </div>
							<div class="form-group">
		                        <label>手机号码：</label>
								<input type="text" name="vcPhone" value="$!{query.vcPhone}">
		                    </div>
		                    <div class="form-group">
		                        <button class="btn btn-xs btn-primary" type="submit">查询</button>
		                    </div>
		                </form>
		            </div>
                    <!--<div class="col-sm-1">
                        <a href="$!{www}/smsrecord/new" class="btn btn-success btn-xs">添加</a>
					</div>-->
		        </div>
		        <table class="table table-striped table-bordered table-hover dataTable">
		            <thead>
		            <tr>
		                <th>ID</th>
		                <th>手机号码</th>
		                <th>短信内容</th>
		                <th>提交状况</th>
		                <!--<th>提交返回结果</th>-->
		                <th>发送状况</th>
		                <th>短信接口</th>
						<th>提交时间</th>
		                <th>发送时间</th>
		               
		            </tr>
		            </thead>
		            <tbody>
		                #foreach($row in $result.rows)
		                <tr>
		                	<td> $!{row.id} </td>
		                	<td> ${row.vcPhone.substring(0,3)}****${row.vcPhone.substring(7,11)} </td>
							<td> $!{row.vcContent} </td>
		                	<td> #if($row.istate==1)成功#elseif($row.istate==2) 失败#end</td>
		                	<!--<td>$!{row.sResult}</td>-->	
		                	<td>#if($row.sState=="0" || $row.sState=="DELIVRD")成功#else $!{row.sState}#end</td>
		                	<td> #if($row.sinterface==1)漫道#elseif($row.sinterface==2) 建周#elseif($row.sinterface==3)亿美#elseif($row.sinterface==4)示远 #else 暂无 #end </td>
		                	<td> $dateTool.format("yyyy-MM-dd HH:mm:ss",$!{row.dtCreate}) </td>
							<td> $dateTool.format("yyyy-MM-dd HH:mm:ss",$!{row.dtSend}) </td>
		                </tr>
		                #end
		            </tbody>
		        </table>
		        <div class="row">
		            <div class="col-sm-12">
		                <div class="dataTables_paginate paging_bootstrap">
		                    <ul class="pagination">
		                    	#set($pageRequestURL="$!{www}/smsrecord?pageIndex=[PAGINATION_PAGE_INDEX]#if($!{result.queryParameters})&$!{result.queryParameters}#end")
		                    	#paginationHelper($result $pageRequestURL)
		                    </ul>
		                </div>
		            </div>
		       </div>
		       
		    </div>
		</div>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->