			function changeType(type){
				if(type==5){
				$("#numDiscount-div").css("display","");
				}else{
				$("#numDiscount-div").css("display","none");
				}
				if(type==7){
					$("#interest-div").css("display","");
				}else{
					$("#interest-div").css("display","none");
				}
				if(type!="1" && type!="2"){
					if($('input:radio[name="iIsAutoOpenSubject"]:checked').val()=="1"){
						clkAutoOpen($("#iIsAutoOpenSubject1"));
					}
				}
			}
			function tijiao(){
				var itype = document.getElementById("itype");
				var vcName = document.getElementById("vcName");
				var numTotalFinancing = document.getElementById("numTotalFinancing");
				var ivalueDateType = document.getElementById("ivalueDateType");
				var dtTrailerStr = document.getElementById("dtTrailerStr");
				var dtCollectStartStr = document.getElementById("dtCollectStartStr");
				var dtCollectEndStr = document.getElementById("dtCollectEndStr");
			
				var numInterestRate = document.getElementById("numInterestRate");
				var numRewardRate = document.getElementById("numRewardRate");
				var numManagementRate = document.getElementById("numManagementRate");
				var numPlatFormFee = document.getElementById("numPlatFormFee");
				var numBond = document.getElementById("numBond");
				var numWithdrawFeestr = document.getElementById("numWithdrawFeestr");
				var numWithdrawFee = document.getElementById("numWithdrawFee");
				var numDiscount = document.getElementById("numDiscount");
				
				var iplatFormFeeType = document.getElementById("iplatFormFeeType");
				var ibondType = document.getElementById("ibondType");
				var  iInterestConfigID = document.getElementById("iInterestConfigID");
				
				if(itype.value == ""){
					alert("标的类型不能为空！");
					itype.focus();
					return false;
				}
				if(itype.value == 5 && numDiscount.value == "" ){
					alert("标的平台-贴现支出不能为空！");
					numDiscount.focus();
					return false;
				}
				if(itype.value!=5){
					numDiscount.value = "0.00";
				}
				
				if(vcName.value == ""){
					alert("标的名称不能为空！");
					vcName.focus();
					return false;
				}
				
				var reg = /^\d+(\.{0,1}\d+){0,1}$/;
				if(itype.value == 5 && numDiscount.value != ""&&!reg.test(numDiscount.value) ){
					alert("标的平台-贴现支出不符合规则！");
					numDiscount.focus();
					return false;
				}
				
				if(itype.value == 7 && iInterestConfigID.value == ""){
					alert("多财标准不能为空！");
					return false;
				}
				
				if(numInterestRate.value == ""){
					alert("预计年利率不能为空！");
					numInterestRate.focus();
					return false;
				}
				if(!reg.test(numInterestRate.value)){
					alert("预计年利率不符合规则！");
					numInterestRate.focus();
					return false;
				}
				if(numRewardRate.value != ""){
					if(!reg.test(numRewardRate.value)){
						alert("奖励年利率不符合规则！");
						numRewardRate.focus();
						return false;
					}
				}
				if(numManagementRate.value == ""){
					alert("管理费利率不能为空！");
					numManagementRate.focus();
					return false;
				}
				if(!reg.test(numManagementRate.value)){
					alert("管理费利率不符合规则！");
					numManagementRate.focus();
					return false;
				}
				if(numTotalFinancing.value == ""){
					alert("融资金额不能为空！");
					numTotalFinancing.focus();
					return false;
				}
				if(ivalueDateType.value == ""){
					alert("起息日不能为空！");
					ivalueDateType.focus();
					return false;
				}
				if(numPlatFormFee.value == ""){
					alert("平台服务费率不能为空！");
					numPlatFormFee.focus();
					return false;
				}
				if(!reg.test(numPlatFormFee.value)){
					alert("平台服务费率不符合规则！");
					numPlatFormFee.focus();
					return false;
				}
				if(iplatFormFeeType.value == ""){
					alert("平台服务费率类型不能为空！");
					iplatFormFeeType.focus();
					return false;
				}
				if(numBond.value == ""){
					alert("保证金费率不能为空！");
					numBond.focus();
					return false;
				}
				if(!reg.test(numBond.value)){
					alert("保证金费率不符合规则！");
					numBond.focus();
					return false;
				}
				if(ibondType.value == ""){
					alert("保证金类型不能为空！");
					ibondType.focus();
					return false;
				}
				if(numWithdrawFeestr.value == ""){
					alert("借款人提现费率不能为空！");
					numWithdrawFeestr.focus();
					return false;
				}
				if(!reg.test(numWithdrawFeestr.value)){
					alert("借款人提现费率不符合规则！");
					numWithdrawFeestr.focus();
					return false;
				}
				//linjie  新增“是否开启自动开标”提交前的验证逻辑  begin
				var isauto=$("input[name='iIsAutoOpenSubject']:checked").val();
				if(isauto==1){
					if($("#numPeriod").val()==""){
						alert("募集期限不能为空！");
						$("#numPeriod").focus();
						return false;
					}
				}else{
					if(dtTrailerStr.value == ""){
						alert("预告开始时间不能为空！");
						dtTrailerStr.focus();
						return false;
					}
					if(dtCollectStartStr.value == ""){
						alert("募集开始时间不能为空！");
						dtCollectStartStr.focus();
						return false;
					}
					if(dtCollectEndStr.value == ""){
						alert("募集结束时间不能为空！");
						dtCollectEndStr.focus();
						return false;
					}
				}
				if($("#iSubjectPeriodGradeID").val()==""){
					alert("请选择期限等级！");
					$("#iSubjectPeriodGradeID").focus();
					return false;
				}
				//linjie   end
				numWithdrawFee.value=numWithdrawFeestr.value/100;
				var inputForm_subject = document.getElementById("inputForm_subject");
				inputForm_subject.submit();
			}
            
			//linjie 点击radio，切换“是否开启自动开标”不同情况的显示字段
			function clkAutoOpen(obj){
				var div0=document.getElementById("autoOpen0");
				var div1=document.getElementById("autoOpen1");
				var val=obj.value;
				if(typeof(val) == "undefined"){
					val=obj.val();
				}
				if(val=="0"){
					div0.style.display="";
					div1.style.display="none";
				}else if(val=="1"){
					if($("#itype").val()!=""){
						if($("#itype").val()!="1" && $("#itype").val()!="2"){
							$(":radio[name=iIsAutoOpenSubject]:not(:checked)").prop("checked", true);
							alert("抱歉！只有【普通标】与【新手标】可以设置自动开标！");
							div0.style.display="";
							div1.style.display="none";
							return;
						}
					}
					div0.style.display="none";
					div1.style.display="";
				}else{
					alert("获取选择项失败");
				}
			}
