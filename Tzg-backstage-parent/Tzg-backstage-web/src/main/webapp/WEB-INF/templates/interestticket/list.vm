<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li class="active">加息券管理</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>
	<div class="page-content">
		<div class="page-header">
		    <h1>加息券管理</h1>
		</div>
		<div class="container-fluid">
		    <div class="dataTables_wrapper">
		        <div class="row">
		            <div class="col-sm-10">
		               	 <h5><b>加息券列表</b></h5>
		            </div>
                    <div class="col-sm-2">
                        <a href="$!{www}/interestticket/new" class="btn btn-success btn-xs">新增加息券</a>
                    </div>
		        </div>
		        <table class="table table-striped table-bordered table-hover dataTable">
		            <thead>
		            <tr>
		            	<th>创建时间</th>
		            	<th>更新时间</th>
		                <th>名称</th>
		                <th>加息值</th>
		                <th>最长加息时间</th>
		                <th>最大加息本金</th>
		                <th>有效期</th>
		                <th>操作</th>
		            </tr>
		            </thead>
		            <tbody>
		                #foreach($row in $result.rows)
		                <tr>
		                	<td> $!{row.dtCreateStr} </td>
		                	<td> $!{row.dtModifyStr} </td>
		                	<td> $!{row.vcName} </td>
		                	<td> $!{row.numInterestRate}%</td>
		                	<td> $!{row.imaxInterestTime}天</td>
		                	<td> $!{row.numMaxInterestRate}元</td>
		                	<td> $!{row.iexpired}天</td>
		                    <td><a href="$!{www}/interestticket/find?id=$!{row.id}">修改</a>   ｜
		                        <a onclick="delcfm()" href="$!{www}/interestticket/delete?id=$!{row.id}">删除</a>
		                    </td>
		                </tr>
		                #end
		            </tbody>
		        </table>
		        <div class="row">
		            <div class="col-sm-12">
		                <div class="dataTables_paginate paging_bootstrap">
		                    <ul class="pagination">
		                    	#set($pageRequestURL="$!{www}/interestticket/list?pageIndex=[PAGINATION_PAGE_INDEX]#if($!{result.queryParameters})&$!{result.queryParameters}#end")
		                    	#paginationHelper($result $pageRequestURL)
		                    </ul>
		                </div>
		            </div>
		       </div>
		       
		    </div>
		</div>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->