<script src="${www}/js/my97DatePicker/WdatePicker.js"></script>
<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li class="active">充值提现申请记录</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>
	<div class="page-content">
		<div class="page-header">
		    <h1>充值提现申请记录</h1>
		</div>
		<div class="container-fluid">
		    <div class="dataTables_wrapper">
		        <div class="row">
		            <div class="col-sm-11">
		                <form class="form-inline" role="form">
		                    <div class="form-group">
		                        <label>日期：</label>
		                        <input type="text" name="dtCreateBeginFuzzy"  id="dtCreateBeginFuzzy" onFocus="WdatePicker({lang:'zh-cn'})" 
		                        value="$!{query.dtCreateBeginFuzzy}" class="input-date">
								-
								<input type="text" name="dtCreateEndFuzzy" id="dtCreateEndFuzzy" onFocus="WdatePicker({lang:'zh-cn'})" 
								value="$!{query.dtCreateEndFuzzy}" class="input-date">
		                    </div>
		                    <div class="form-group">
		                        <button class="btn btn-xs btn-primary" type="submit">搜索</button>
		                    </div>
		                </form>
		            </div>
		        </div>
		        <table class="table table-striped table-bordered table-hover dataTable">
		            <thead>
		            <tr>
		                <th>资金来源</th>
		                <th>操作类型</th>
		                <th>金额</th>
		                <th>申请提交人</th>
		                <th>当前审批流程</th>
		                <th>审核状态</th>
		                <th>创建时间</th>
		                <th>操作</th>
		            </tr>
		            </thead>
		            <tbody>
		                #foreach($row in $result.rows)
		                <tr>
		                	<td>#if($row.isourcetype==1) 平台
		                		#elseif($row.isourcetype==2) 备用金
		                		#end
		                	</td>
		                	<td>#if($row.ioperatetype==1) 充值
		                		#elseif($row.ioperatetype==2) 提现
		                		#end</td>
		                	<td> $!{row.numamount} </td>
		                	<td> $!{row.vcRealName} </td>
		                	<td>#if($row.vcCurrentFlow==1) 总经理
		                		#elseif($row.vcCurrentFlow==2) 部门经理
		                		#elseif($row.vcCurrentFlow==3) 主管
		                		#elseif($row.vcCurrentFlow==4) 员工
		                		#end
		                	</td>
		                	<td>#if($row.istate==1) 刚录入
		                		#elseif($row.istate==2) 已提交
		                		#elseif($row.istate==3) 审核通过
		                		#elseif($row.istate==4) 审核未通过
		                		#end </td>
		                	<td> $!{row.dtCreateStr} </td>
		                    <td>
		                    	#if($row.istate==3 || $row.istate==4)
		                    		<a href="$!{www}/companyfinancialaudit/find?id=$!{row.id}">查看</a>
		                    	#elseif($row.istate==2)
		                    		#foreach($audit in $auditList)
                          				#if($!{row.vcCurrentFlow}==$audit)
                          					<a href="$!{www}/companyfinancialaudit/toApproval?id=$!{row.id}">审核</a>   
                          				#end
                          			#end
		                    	#elseif($row.istate==1)
		                    		<a href="#" onclick="submitF($!{row.id})">提交</a>
		                    	#end
		                    </td>
		                </tr>
		                #end
		            </tbody>
		        </table>
		        <div class="row">
		            <div class="col-sm-12">
		                <div class="dataTables_paginate paging_bootstrap">
		                    <ul class="pagination">
		                    	#set($pageRequestURL="$!{www}/companyfinancialaudit?pageIndex=[PAGINATION_PAGE_INDEX]#if($!{result.queryParameters})&$!{result.queryParameters}#end")
		                    	#paginationHelper($result $pageRequestURL)
		                    </ul>
		                </div>
		            </div>
		       </div>
		       
		    </div>
		</div>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->
<script>
	function submitF(id){
		var flag = confirm("确认要提交该申请吗？");
		if (flag == true){
			window.location.href="${www}/companyfinancialaudit/submitAudit?id="+id;
		}
	}
	function delFun(id){
    	var flag = confirm("确认删除该申请吗？")
    	if (flag == true){
    		window.location.href="${www}/companyfinancialaudit/delete?id=" + id;
    	}   
    }
</script>