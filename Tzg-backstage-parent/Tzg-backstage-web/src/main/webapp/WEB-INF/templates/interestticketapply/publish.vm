<script src="${www}/js/my97DatePicker/WdatePicker.js"></script>
<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
        
        </script>
        
         <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li>
                <a href="${www}/interestticketapply/list">加息券列表</a>
            </li>
            <li class="active">
            	查看
            </li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>
        
    <div class="page-content">
	        <div class="page-header">   
	        	<h1>查看
	        </h1>	
	</div>
	    	<div class="container-fluid">
	      

	<div class="tab-content">
	    <form action="${www}/interestticketapply/publish" method="post" class="form-horizontal">
                 <div class="row">
                   <div class="col-xs-12 col-md-6">
                        <div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">活动名称：</label>
                            <div class="col-md-8 col-sm-8">
                                $!{param.vcInterestTicketAccountName}
                            </div>
                        </div>
						  <div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">申请人：</label>
                            <div class="col-md-8 col-sm-8">
                                $!{param.vcApplyAccountName}
                            </div>
                        </div>
						 <div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">申请类型：</label>
                            <div class="col-md-8 col-sm-8">
                              #if($param.iapplyType && $param.iapplyType ==1)新增#end #if($param.iapplyType && $param.iapplyType ==2) 追加 #end
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">申请时间：</label>
                            <div class="col-md-8 col-sm-8">
							    #if( $param.dtCreate)$dateTool.format("yyyy-MM-dd", $!{param.dtCreate}) #end
                            </div>
                        </div>
                       <div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">活动开始时间：</label>
                            <div class="col-md-8 col-sm-8">
                                #if( $param.dtStart)$dateTool.format("yyyy-MM-dd", $!{param.dtStart}) #end
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">活动结束时间：</label>
                            <div class="col-md-8 col-sm-8" >
								#if( $param.dtEnd)$dateTool.format("yyyy-MM-dd", $!{param.dtEnd}) #end
                            </div>
                        </div>
       
						<div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">加息券最终失效日：</label>
                            <div class="col-md-8 col-sm-8" >
    							$!{param.dtExpireStr}
                            </div>
                        </div>
						<div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">最大预算：</label>
                            <div class="col-md-8 col-sm-8" >
    							$!{param.numApplyAmt}
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">预警金额：</label>
                            <div class="col-md-8 col-sm-8" >
    							$!{param.numAlertAmt}
                            </div>
                        </div>
                         <div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">是否已发预警：</label>
                            <div class="col-md-8 col-sm-8" >
                            #if($param.isSendSms == 1) 
    							未发
    						#elseif($param.isSendSms == 2) 
    							已发
    						#end
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">超预算补发：</label>
                            <div class="col-md-8 col-sm-8" >
                            #if($param.iSuperBudget == 1) 
    							支持超预算发放
    						#elseif($param.iSuperBudget == 2)
    							不支持超预算发放
    						#end
                            </div>
                        </div>
                        
                        
						  <div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">发放方式：</label>
                            <div class="col-md-8 col-sm-8" >
								#if($param.iactionType== 1)
                            	 	定向发放
                            	#elseif($param.iactionType== 2) 
                            		专项活动
								#end
                            </div>
                        </div>
                         #if($param.iactionType== 1)
						  <div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">发放规则：</label>
                            <div class="col-md-8 col-sm-8" >
								
	    							<table style="width:450px;border:solid #000 1px;" >
	                                    <tr>
											<th style="border:solid #000 1px;">加息额</th>
											<th style="border:solid #000 1px;">最长加息时间</th>
											<th style="border:solid #000 1px;">最大加息额度</th>
											<th style="border:solid #000 1px;">单张价值</th>
											<th style="border:solid #000 1px;">总数量</th>
											<th style="border:solid #000 1px;">总价值</th>
										</tr>
										 #foreach($row in $respondList)
										<tr>
											<td style="border:solid #000 1px;" >
												$!{row.numInterestRate} %加息券
	                                        </td>
											<td style="border:solid #000 1px;">
												$!{row.imaxInterestTime} 天
	                                        </td>
	                                        <td style="border:solid #000 1px;">
												$!{row.numMaxInterestRate} 元
	                                        </td>
	                                        <td style="border:solid #000 1px;">
												$!{row.numWorth} 元
	                                        </td>
	                                        <td style="border:solid #000 1px;">
												$!{row.itotalCount} 张
	                                        </td>
	                                        <td style="border:solid #000 1px;">
												$!{row.totalAmount} 元
	                                        </td>
	                                    </tr>
	                                    #end
                               		</table>
								
                            </div>
                        </div>
                        #end
                         #if($!{result.rows.size()}>0) 
                        <div class="form-group">
                            <label class="col-md-4 col-sm-2 control-label">审核情况：</label>
                            <div class="col-md-8 col-sm-8" >
								
	    							<table style="border:solid #000 1px;">
	                                    <tr>
											<th style="border:solid #000 1px;">审核人</th>
											<th style="border:solid #000 1px;">审核时间</th>
											<th style="border:solid #000 1px;">审核状态</th>
											<th style="border:solid #000 1px;">审核意见</th>
										</tr>
										 #foreach($row in $result.rows)
										<tr>
											<td style="border:solid #000 1px;" >
												 ${row.vcApproverName}
	                                        </td>
											<td style="border:solid #000 1px;">
												${row.dtApproveStr}
	                                        </td>
	                                        <td style="border:solid #000 1px;">
												#if(${row.istatus}==1) 通过 #end
							                	#if(${row.istatus}==2) 终止 #end
							                	#if(${row.istatus}==3) 退回 #end
	                                        </td>
	                                        <td style="border:solid #000 1px;">
												${row.vcApproveIdea}
	                                        </td>
	                                    </tr>
	                                    #end
                               		</table>
                            </div>
                        </div>
                        #end
                     </div>
                </div>
                 <div class="col-md-8 col-sm-8">
			          <input type="hidden" class="col-sm-8" name="id" value="$!{param.id}"/>	 	
			    </div>
			    <div class="clearfix form-actions">
                <div class="col-sm-offset-2 col-md-9">
                	<button class="btn btn-info" type="submit">
                        <i class="icon-ok bigger-110"></i>发布
                    </button>
                     &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		             <button class="btn btn-info" type="button" onclick="javascript:history.go(-1);">
		                <i class="icon-arrow-left bigger-110"></i>返回
		            </button>
                </div>
            </div>
            </form>
        </div>	
     </div><!-- /.page-content -->
</div><!-- /.main-content -->