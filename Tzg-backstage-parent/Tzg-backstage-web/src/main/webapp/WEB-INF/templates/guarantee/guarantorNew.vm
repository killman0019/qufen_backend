<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>
            <li>
                <a href="${www}/guarantee">担保机构列表</a>
            </li>
            <li class="active">添加登陆账户</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>

    <div class="page-content">
        <div class="page-header">
            <h1>添加登陆账户</h1>
        </div>
        
                				#if(${msg})
        	<div>
			#if(${success})
        		<li class="text-success">$!{msg}</li>
   			 #else
        		<li class="text-danger">$!{msg}</li>
   			 #end    	
   			</div>
   		#end	
        
        <form action="${www}/guarantee/guarantor/save" method="post" class="form-horizontal">
        	<input type="hidden" name="itype" value="3"/>
        	<input type="hidden" name="guaranteeId" value="$!{guaranteeId}"/>
            <div class="row">
                <div class="col-xs-12 col-md-4">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label"><span class="red">*</span>手机号：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcPhone" value="$!{param.vcPhone}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label"><span class="red">*</span>姓名：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcName" value="$!{param.vcName}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label"><span class="red">*</span>身份证号码：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcCardCode" value="$!{param.vcCardCode}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">登陆账号：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcLoginName" value="$!{param.vcLoginName}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">邮箱账号：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcEmail" value="$!{param.vcEmail}"/>
                        </div>
                    </div>
               	</div>
            </div>
            <div class="clearfix form-actions">
                <div class="col-sm-offset-2 col-md-9">
                    <button class="btn btn-info" type="submit">
                        <i class="icon-ok bigger-110"></i> 提交
                    </button>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		            <button class="btn" type="button" onclick="javascript:history.go(-1);">
		                <i class="icon-arrow-left bigger-110"></i>返回
		            </button>
                </div>
            </div>
</form>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->