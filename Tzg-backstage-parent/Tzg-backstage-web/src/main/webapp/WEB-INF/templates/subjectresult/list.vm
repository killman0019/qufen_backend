<script src="${www}/js/my97DatePicker/WdatePicker.js"></script>
<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li class="active"><a href="${www}/subjectresult/list">数据查询</a></li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>
	<div class="page-content">
		<div class="page-header">
		    <h1>数据查询</h1>
		</div>
		<div class="container-fluid">
		    <div class="dataTables_wrapper">
		        <div class="row">
		            <div class="col-sm-11">
		                <form class="form-inline" role="form">
		                    <div class="form-group">
		                        <label>标的名称：</label>
		                        <input type="text" name="vcName" value="$!{query.vcName}">
		                    </div>
		                    <div class="form-group">
		                        <label>实际募集结束时间：</label>
		                        <input type="text"  name="dtCollectEndBegin" style="width:150px" id="dtCollectEndBegin" onFocus="WdatePicker({lang:'zh-cn',dateFmt:'yyyy-MM-dd HH:mm:ss'})" value="$!{query.dtCollectEndBegin}" class="input-date">
								-
								<input type="text" name="dtCollectEndEnd"  style="width:150px" id="dtCollectEndEnd" onFocus="WdatePicker({lang:'zh-cn',dateFmt:'yyyy-MM-dd HH:mm:ss'})" value="$!{query.dtCollectEndEnd}" class="input-date">
		                 	</div>
		                    <div class="form-group">
		                        <button class="btn btn-xs btn-primary" type="submit">查询</button>
		                    </div>
		                </form>
		            </div>
		        </div>
		        <table class="table table-striped table-bordered table-hover dataTable">
		            <thead>
		            <tr>
		            	<th>项目名称</th>
		                <th>标的名称</th>
		                <th>标的状态</th>
		                <th>实际募集结束时间</th>
		                <th>起始利率</th>
		                <th>融资金额</th>
		                <th>实际募集<br />金额</th>
		                <th>募集期利息</th>
		                <th>利息</th>
		                <th>协议募集期<br />利息</th>
		                <th>协议支付<br />利息</th>
		              	<th>多财贴息</th>
		                <th>多财赚息</th>
		                <th>操作</th>
		            </tr>
		            </thead>
		            <tbody>
		                #foreach($row in $result.rows)
		                <tr>
		                	<td> $!{row.projectName} </td>
		                	<td> $!{row.vcName} </td>
		                	<td> #if($row.istate =='1')新录入#end
			                     #if($row.istate =='2')待审核 #end
			                     #if($row.istate =='3')退回 #end
			                     #if($row.istate =='4')作废 #end
			                     #if($row.istate =='5')审核通过#end
			                     #if($row.istate =='6')待预告 #end
			                     #if($row.istate =='10')待开标#end
			                     #if($row.istate =='11')投标中 #end
			                     #if($row.istate =='12')还款中#end
			                     #if($row.istate =='13')结束 #end
			                     #if($row.istate =='14')等待划款 #end
		                     </td>
		                	<td> $!{row.dtCollectEndStr} </td>
		                	<td> $!{row.totalRate}%</td>
		                	<td> $!{row.numTotalFinancing} </td>
		                	<td> $!{row.numActualFinancing} </td>
		                	<td> $!{row.numCollectInterest} </td>
		                	<td> $!{row.numInterest} </td>
		                	<td> $!{row.numAgreementCollectInterest} </td>
		                	<td> $!{row.numAgreementInterest} </td>
		                	<td> $!{row.numMminterestLose} </td>
		                	<td> $!{row.numMminterestEarn} </td>
		                    <td><a href="$!{www}/subjectresult/investlist?subjectid=$!{row.id}">查看</a>
		                    </td>
		                </tr>
		                #end
		            </tbody>
		        </table>
		        <div class="row">
		            <div class="col-sm-12">
		                <div class="dataTables_paginate paging_bootstrap">
		                    <ul class="pagination">
		                    	#set($pageRequestURL="$!{www}/subjectresult/list?pageIndex=[PAGINATION_PAGE_INDEX]#if($!{result.queryParameters})&$!{result.queryParameters}#end")
		                    	#paginationHelper($result $pageRequestURL)
		                    </ul>
		                </div>
		            </div>
		       </div>
		       
		    </div>
		</div>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->