<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li class="active"><a href="${www}/subjectresult/list">数据查询</a></li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>
	<div class="page-content">
		<div >
		   <b>标的名称: $!{subjectresult.vcName}</b> 起始利率$!{subjectresult.totalRate}%  实际募集金额 $!{subjectresult.numActualFinancing} 元
		</div>
		<div class="container-fluid">
		    <div class="dataTables_wrapper">
		        <div class="row">
		            <div class="col-sm-11">
		                <form class="form-inline" role="form">
		                    <div class="form-group">
		                        <label>利率等级：</label>
		                        <select name="configId" >
		                        	<option value="">--请选择--</option>
		                        	#foreach($row in $configdetailResult.rows)
		                        		<option  #if($!{query.iInterestConfigDetailId}==$!{row.id}) selected="selected" #end value="$!{row.id}">$!{row.vcLevel}</option>
		                        	#end
		                        </select>
								<input type="hidden" value="$!{query.subjectid}" name="subjectid"/>
		                    </div>
		                    <div class="form-group">
		                        <button class="btn btn-xs btn-primary" type="submit">查询</button>
		                    </div>
		                </form>
		            </div>
		            <div class="col-sm-1">
                        <a href="$!{www}/subjectresult/list" class="btn btn-success btn-xs">返回</a>
                    </div>
		        </div>
		        <table class="table table-striped table-bordered table-hover dataTable">
		            <thead>
		            <tr>
		                <th>投资人ID</th>
		                <th>投资时间</th>
		                <th>投资金额</th>
		                <th>利率等级</th>
		                <th>投资人实际利率</th>
		                <th>募集期利息</th>
		                <th>利息</th>
		                <th>投资人总利息</th>
		            </tr>
		            </thead>
		            <tbody>
		                <th>汇总</th>
		                <th>--</th>
		                <th>$!{numInvestTotalAmt}</th>
		                <th>--</th>
		                <th>--</th>
		                <th>$!{numActualExtraInterestTotal}</th>
		                <th>$!{numActualInterestTotal}</th>
		                <th>$!{numInerestTotal}</th>
		            
		                #foreach($row in $result.rows)
		                <tr>
		                	<td> $!{row.iloginAccountId} </td>
		                	<td> $!{row.dtInvestStr} </td>
		                	<td> $!{row.numInvest} </td>
		                	<td> $!{row.iInterestConfigDetailName} </td>
		                	<td> $!{row.numTotalRate}% </td>
		                	<td> $!{row.numRaiseInterestTotal} </td>
		                	<td> $!{row.numActualInterestTotal} </td>
		                	<td> $!{row.numInterestTotal} </td>
		                    </td>
		                </tr>
		                #end
		            </tbody>
		        </table>
		        <div class="row">
		            <div class="col-sm-12">
		                <div class="dataTables_paginate paging_bootstrap">
		                    <ul class="pagination">
		                    	#set($pageRequestURL="$!{www}/subjectresult/investlist?subjectid=$!{query.subjectid}&pageIndex=[PAGINATION_PAGE_INDEX]#if($!{result.queryParameters})&$!{result.queryParameters}#end")
		                    	#paginationHelper($result $pageRequestURL)
		                    </ul>
		                </div>
		            </div>
		       </div>
		       
		    </div>
		</div>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->