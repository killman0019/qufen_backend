<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li>
                <a href="${www}/loginaccount">客户列表</a>
            </li>
            <li class="active">客户详情</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>

    <div class="page-content">

        <div class="page-header">
            <h1>客户详情</h1>
        </div>
        <form action="#" method="post" class="form-horizontal">
            <div class="row">
                <div class="col-xs-12 col-md-4">
                   <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">状态：</label>
                        <div class="col-md-8 col-sm-8">
                        	#if($!{param.ivalid}==1)正常
		                	#else <font color="red">已冻结</font>
		                	#end
                        </div>
                    </div>
               
                      <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">类型：</label>
                        <div class="col-md-8 col-sm-8">
                            	#if($!{param.itype}==1)借款人
			                	#elseif($!{param.itype}==2)投资人
			                	#elseif($!{param.itype}==3)担保人
			                	#elseif($!{param.itype}==4)平台账户
			                	#elseif($!{param.itype}==5)渠道
			                	#end
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">姓名：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.vcName}
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">证件号码：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.vcCardCode}
                        </div>
                    </div>                    
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">手机号：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.vcPhone}
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">登陆账号：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.vcLoginName}
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">邮箱账号：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.vcEmail}
                        </div>
                    </div>
                    
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">手机认证：</label>
                        <div class="col-md-8 col-sm-8">
                        	#if($!{param.iverifyPhone}==1)已认证
		                	#else未认证 
		                	#end
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">邮箱认证：</label>
                        <div class="col-md-8 col-sm-8">
                        	#if($!{param.iverifyEmail}==1)已认证
		                	#else未认证 
		                	#end
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">实名认证：</label>
                        <div class="col-md-8 col-sm-8">
                        	#if($!{param.iverifyIdcard}==1)已认证
		                	#else未认证 
		                	#end
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">密码：</label>
                        <div class="col-md-8 col-sm-8">
                        	#if($!param.iOriginal && $!param.iOriginal==1)已修改
		                	#else未修改
		                	#end
                        </div>
                    </div>                    
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">账号来源：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.vcSource}
                        </div>
                    </div>
                     <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">注册设备：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.vcRegisterDevice}
                        </div>
                    </div>                   
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">首次投资时间：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.dtFirstInvestStr}
                        </div>
                    </div>
                     <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">推荐码：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.vcRecommendCode}
                        </div>
                    </div>                   
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">创建的时间：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.dtCreateStr}
                        </div>
                    </div>
                                       
                 </div>
            </div>
            <div class="clearfix form-actions">
                <div class="col-sm-offset-2 col-md-9">
		            <button class="btn" type="button" onclick="javascript:history.go(-1);">
		                <i class="icon-arrow-left bigger-110"></i>返回
		            </button>
                </div>
            </div>
    </div>
</div>

