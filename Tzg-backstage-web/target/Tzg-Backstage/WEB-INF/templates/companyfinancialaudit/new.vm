<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li>
                <a href="${www}/companyfinancialaudit">Companyfinancialaudit列表</a>
            </li>
            <li class="active">新增Companyfinancialaudit</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>

    <div class="page-content">
        <div class="page-header">
            <h1>新增Companyfinancialaudit</h1>
        </div>
        <form action="${www}/companyfinancialaudit/save" method="post" class="form-horizontal">
            <div class="row">
                <div class="col-xs-12 col-md-4">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">id：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="id" value="$!{param.id}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">--1：平台；2：备用金：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="isourcetype" value="$!{param.isourcetype}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">1-充值 2-提现：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="ioperatetype" value="$!{param.ioperatetype}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">金额：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="numamount" value="$!{param.numamount}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">后台登陆人员的账号主键：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="iapplyAccountId" value="$!{param.iapplyAccountId}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">当前审批流程：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcCurrentFlow" value="$!{param.vcCurrentFlow}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">ApprovalRating主键ApprovalRating主键：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcApprovalFlow" value="$!{param.vcApprovalFlow}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">1 -- 刚录入；\r\n2 -- 已提交；\r\n3 -- 审核通过；\r\n4 -- 审核未通过\r\n\r\n--------\r\n审核通过时：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="istate" value="$!{param.istate}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">备注：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcMemo" value="$!{param.vcMemo}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">创建时间：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="dtCreate" value="$!{param.dtCreate}"/>
                        </div>
                    </div>
                    <div class="red">$!{msg}</div>
               	</div>
            </div>
            <div class="clearfix form-actions">
                <div class="col-sm-offset-2 col-md-9">
                    <button class="btn btn-info" type="submit">
                        <i class="icon-ok bigger-110"></i> 提交
                    </button>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		            <button class="btn btn-info" type="button" onclick="javascript:history.go(-1);">
		                <i class="icon-arrow-left bigger-110"></i>返回
		            </button>
                </div>
            </div>
</form>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->