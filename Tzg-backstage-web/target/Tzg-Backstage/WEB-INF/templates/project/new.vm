<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li>
                <a href="${www}/project">项目列表</a>
            </li>
            <li class="active">新增项目</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>

    <div class="page-content">
        <div class="page-header">
            <h1>新增项目</h1>
        </div>
        <form action="${www}/project/save" method="post" class="form-horizontal">
            <div class="row">
                <div class="col-xs-12 col-md-4">
                 
                 <!--
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">项目图标：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="projectIcon" value="$!{param.projectIcon}"/>
                        </div>
                    </div>
                  -->
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">代币名称</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="projectCode" value="$!{param.projectCode}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">英文名称</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="projectEnglishName" value="$!{param.projectEnglishName}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">中文名称</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="projectChineseName" value="$!{param.projectChineseName}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">官网</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="websiteUrl" value="$!{param.websiteUrl}"/>
                        </div>
                    </div>
                   <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">是否上市(0-未上市,1-已上市)</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="listed" value="$!{param.listed}"/>
                        </div>
                    </div>        
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">发行时间</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="issueDateStr" value="$!{param.issueDateStr}"/>
                        </div>
                    </div>        
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">发行量</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="issueNum" value="$!{param.issueNum}"/>
                        </div>
                    </div>       
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">白皮书地址</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="whitepaperUrl" value="$!{param.whitepaperUrl}"/>
                        </div>
                    </div>             
                    
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">项目分类名称</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="projectTypeName" value="$!{param.projectTypeName}"/>
                        </div>
                    </div>                    
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">项目分类Id：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="projectTypeId" value="$!{param.projectTypeId}"/>
                        </div>
                    </div>                    
                                                                    
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">项目简单描述</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="projectSignature" value="$!{param.projectSignature}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">项目描述：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="textarea" class="col-sm-12" col=4 name="projectDesc" value="$!{param.projectDesc}"/>
                        </div>
                    </div>
                    
                    
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">提交人信息</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="hidden" class="col-sm-12" name="submitUserId" value="$!{param.submitUserId}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">联系方式</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="submitUserContactInfo" value="$!{param.submitUserContactInfo}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">利益相关(1-普通用户;2-利益相关;3-项目方;4-投资者)</label>
                        <!-- 提交用户类型:1-普通用户;2-利益相关;3-项目方;4-投资者： -->
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="submitUserType" value="$!{param.submitUserType}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">推荐理由</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="submitReason" value="$!{param.submitReason}"/>
                        </div>
                    </div>

                    <div class="red">$!{msg}</div>
               	</div>
            </div>
            <div class="clearfix form-actions">
                <div class="col-sm-offset-2 col-md-9">
                    <button class="btn btn-info" type="submit">
                        <i class="icon-ok bigger-110"></i> 提交
                    </button>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		            <button class="btn btn-info" type="button" onclick="javascript:history.go(-1);">
		                <i class="icon-arrow-left bigger-110"></i>返回
		            </button>
                </div>
            </div>
</form>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->