<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
    	<script src="${www}/js/jquery.maskedinput.min.js"></script>
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
            
            jQuery(function($) {
				$('.input-mask-date').mask('9999-99-99');
			});
            
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="$!{www}/main">主页</a>
            </li>

            <li>
                <a href="${www}/channel">渠道列表</a>
            </li>
            <li class="active">新增渠道</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>

    <div class="page-content">
        <div class="page-header">
            <h1>新增渠道</h1>
        </div>

		        				#if(${msg})
        	<div>
			#if(${success})
        		<li class="text-success">$!{msg}</li>
   			 #else
        		<li class="text-danger">$!{msg}</li>
   			 #end    	
   			</div>
   		#end	

        <form action="${www}/channel/save" method="post" class="form-horizontal">
            <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">名称：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcName" value="$!{param.vcName}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">类型：</label>
                        <div class="col-md-8 col-sm-8">
                        	<select name="itype">
                        		<option value="">请选择</option>
	                            <option value="1" #if($param.itype && $param.itype =='1') selected #end>担保公司</option>
	                            <option value="2" #if($param.itype && $param.itype =='2') selected #end>小额贷款</option>
	                            <option value="3" #if($param.itype && $param.itype =='3') selected #end>资产管理</option>
	                            <option value="4" #if($param.itype && $param.itype =='4') selected #end>投资公司</option>
	                        </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">所在地区：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcArea" value="$!{param.vcArea}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">业务所在地区：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcBusinessArea" value="$!{param.vcBusinessArea}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">授信额度：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="numCredit" value="$!{param.numCredit}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">注册日期：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12 input-mask-date" name="dtRegisterStr" value="$!{param.dtRegisterStr}"  />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">注册资金：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcRegister" value="$!{param.vcRegister}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">资本性质 ：</label>
                        <div class="col-md-8 col-sm-8">
                        	<select name="vcCapital">
                        		<option value="">请选择</option>
	                            <option value="国有企业" #if($param.vcCapital && $param.vcCapital =='国有企业') selected #end>国有企业</option>
	                            <option value="国资背景" #if($param.vcCapital && $param.vcCapital =='国资背景') selected #end>国资背景</option>
	                            <option value="民营企业" #if($param.vcCapital && $param.vcCapital =='民营企业') selected #end>民营企业</option>
	                        </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">企业规模：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcScale" value="$!{param.vcScale}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">经营状况 ：</label>
                        <div class="col-md-8 col-sm-8">
                        	<select name="vcState">
                        		<option value="">请选择</option>
	                            <option value="优良" #if($param.vcState  && $param.vcState =='优良') selected #end>优良</option>
	                            <option value="良好" #if($param.vcState  && $param.vcState =='良好') selected #end>良好</option>
	                            <option value="一般" #if($param.vcState  && $param.vcState =='一般') selected #end>一般</option>
	                            <option value="差" #if($param.vcState  && $param.vcState =='差') selected #end>差</option>
	                            <option value="极差" #if($param.vcState  && $param.vcState =='极差') selected #end>极差</option>
	                        </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">机构评级：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcRate" value="$!{param.vcRate}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">法人代表：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcLegalPerson" value="$!{param.vcLegalPerson}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">董事长：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcChairman" value="$!{param.vcChairman}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">公司电话：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcCompanyTel" value="$!{param.vcCompanyTel}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">机构概述：</label>
                        <div class="col-md-8 col-sm-8">
                        	<textarea name="vcIntroduction" style="margin: 0px; width: 587px; height: 127px;">$!{param.vcIntroduction}</textarea>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">涉诉情况：</label>
                        <div class="col-md-8 col-sm-8">
                        	<textarea name="vcComplain" style="margin: 0px; width: 587px; height: 127px;">$!{param.vcComplain}</textarea>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">管理团队：</label>
                        <div class="col-md-8 col-sm-8">
                        	<textarea name="vcManageGroup" style="margin: 0px; width: 587px; height: 127px;">$!{param.vcManageGroup}</textarea>
                        </div>
                    </div>
                    <div class="page-header">
           					 <h1>联系人信息</h1>
        			</div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">姓名：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcContractName" value="$!{param.vcContractName}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">职务：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcContractLevel" value="$!{param.vcContractLevel}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">电话：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcContractTel" value="$!{param.vcContractTel}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">手机号码：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcContractPhone" value="$!{param.vcContractPhone}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">邮箱：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="vcContractEmail" value="$!{param.vcContractEmail}"/>
                        </div>
                    </div>
               	</div>
            </div>
            <div class="clearfix form-actions">
                <div class="row">
                    <div class="col-md-8">
                        <div class="col-sm-offset-4 col-md-9">
                            <button class="btn btn-info" type="submit">
                                <i class="icon-ok bigger-110"></i>提交
                            </button>
                            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                            <button class="btn" type="button" onclick="javascript:history.go(-1);">
                                <i class="icon-arrow-left bigger-110"></i>返回
                            </button>
                        </div>
                    </div>
                </div>
            </div>
</form>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->
