<script src="${www}/js/my97DatePicker/WdatePicker.js"></script>
<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
			function delFun(id){
            	var flag = confirm("确认删除该记录？")
            	if (flag == true){
            		window.location.href="${www}/investcompanywithdrawalsaudit/delete?id=" + id;
            	}   
            }
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li class="active"> 投资公司提现申请记录</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>
	<div class="page-content">
		<div class="page-header">
		    <h1>投资公司提现申请记录</h1>
		</div>
		<div class="container-fluid">
		    <div class="dataTables_wrapper">
		        <div class="row">
		            <div class="col-sm-8">
		                <form class="form-inline" role="form">
							<input type="hidden" name="operatetype" value="$!{query.ioperatetype}"/>
		                    <div class="form-group">
		                        <label>制单日期：</label>
		                        <input type="text" name="dtCreateBeginFuzzy"  id="dtCreateBeginFuzzy" onFocus="WdatePicker({lang:'zh-cn'})" 
		                        value="$!{query.dtCreateBeginFuzzy}" class="input-date">
								-
								<input type="text" name="dtCreateEndFuzzy" id="dtCreateEndFuzzy" onFocus="WdatePicker({lang:'zh-cn'})" 
								value="$!{query.dtCreateEndFuzzy}" class="input-date">
		                    </div>
		                    <div class="form-group">
		                        <button class="btn btn-xs btn-primary" type="submit">搜索</button>
		                    </div>
		                </form>
		            </div>
                    <div class="col-sm-2">
                        <a href="$!{www}/investcompanywithdrawalsaudit/withdrawcash" class="btn btn-success btn-xs" style="width:100px">提现</a>
                    </div>
		        </div>
		        <table class="table table-striped table-bordered table-hover dataTable">
		            <thead>
		            <tr>
		                <th>制单时间</th>
		                <th>充值金额</th>
		                <th>状态</th>
		                <th>操作</th>
		            </tr>
		            </thead>
		            <tbody>
		                #foreach($row in $result.rows)
		                <tr>
		                	<td>#if($row.dtCreate)$dateTool.format("yyyy-MM-dd",$!{row.dtCreate}) #end</td>
		                	<td>$!{row.numamount} </td>
		                	<td>#if($row.istate==1) 刚录入
		                		#elseif($row.istate==2) 已提交
		                		#elseif($row.istate==3) 审核通过
		                		#elseif($row.istate==4) 审核未通过
		                		#end </td>
		                    <td>
		                    	#if($row.istate==3 || $row.istate==4)
		                    		<a href="$!{www}/investcompanywithdrawalsaudit/find?id=$!{row.id}">查看</a>
		                    	#elseif($row.istate==2)
		                    		#foreach($audit in $auditList)
                          				#if($!{row.vcCurrentFlow}==$audit)
                          					<a href="$!{www}/investcompanywithdrawalsaudit/toApproval?id=$!{row.id}">审核</a>   
                          				#end
                          			#end
		                    	#elseif($row.istate==1)
		                    		<a href="#" onclick="submitF($!{row.id})">提交</a>
									<a href="javascript:delFun(${row.id});">删除</a> 
		                    	#end
		                    </td>
		                </tr>
		                #end
		            </tbody>
		        </table>
		        <div class="row">
		            <div class="col-sm-12">
		                <div class="dataTables_paginate paging_bootstrap">
		                    <ul class="pagination">
		                    	#set($pageRequestURL="$!{www}/investcompanywithdrawalsaudit&pepageIndex=[PAGINATION_PAGE_INDEX]#if($!{result.queryParameters})&$!{result.queryParameters}#end")
		                    	#paginationHelper($result $pageRequestURL)
		                    </ul>
		                </div>
		            </div>
		       </div>
		       
		    </div>
		</div>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->
<script>
	function submitF(id){
		var flag = confirm("确认要提交该申请吗？");
		if (flag == true){
			window.location.href="${www}/investcompanywithdrawalsaudit/submitAudit?id="+id;
		}
	}
	function delFun(id){
    	var flag = confirm("确认删除该申请吗？")
    	if (flag == true){
    		window.location.href="${www}/investcompanywithdrawalsaudit/delete?id=" + id;
    	}   
    }
</script>