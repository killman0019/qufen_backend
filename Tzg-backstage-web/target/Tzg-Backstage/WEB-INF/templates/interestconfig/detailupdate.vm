<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li>
                <a href="${www}/interestconfig/list">利率配置</a>
            </li>
            <li class="active">新增利率区间值</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>

    <div class="page-content">
        <div class="page-header">
            <h1>新增利率区间值</h1>
        </div>
        <form action="${www}/interestconfig/detailupdate" method="post" class="form-horizontal" id="inputForm_interestconfig" >
        	<input type="hidden"  name="id" value="$!{param.id}"/>
        	<input type="hidden"  name="iinterestConfigId" value="$!{param.iinterestConfigId}"/>
            <div class="row">
                <div class="col-xs-12 col-md-5">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">分类名称：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{interestconfig.vcName}
                        </div>
                    </div>
                     <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">等级：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" id="vcLevel"  name="vcLevel" value="$!{param.vcLevel}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">单笔投资大于等于：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text"  id="numMinInvestAmt" name="numMinInvestAmt" value="$!{param.numMinInvestAmt}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">单笔投资小于：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text"  id="numMaxInvestAmt" name="numMaxInvestAmt" value="$!{param.numMaxInvestAmt}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">额外利率：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" id="numExtraInterestRate" name="numExtraInterestRate" value="$!{param.numExtraInterestRate}"/>
                        </div>
                    </div> 
                    <div class="red">$!{msg}</div>
               	</div>
            </div>
            <div class="clearfix form-actions">
                <div class="col-sm-offset-2 col-md-9">
                    <button class="btn btn-info" type="button" onclick="checkSubmit()">
                        <i class="icon-ok bigger-110"></i> 提交
                    </button>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		            <button class="btn btn-info" type="button" onclick="javascript:history.go(-1);">
		                <i class="icon-arrow-left bigger-110"></i>返回
		            </button>
                </div>
            </div>
</form>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->

<script type="text/javascript">
function checkSubmit(){
				var vcLevel = document.getElementById("vcLevel");
				var numMinInvestAmt = document.getElementById("numMinInvestAmt");
				var numMaxInvestAmt = document.getElementById("numMaxInvestAmt");
				var numExtraInterestRate = document.getElementById("numExtraInterestRate");
				if(vcLevel.value == ""){
					alert("等级名称不能为空");
					vcLevel.focus();
					return false;
				}
				var reg = /^[0-9]+([.]{1}[0-9]{1,2})?$/;
				if(numMinInvestAmt.value!=""){
					if(!reg.test(numMinInvestAmt.value)){
						alert("单笔投资大于等于输入正数或小数点后保留两位");
						return false;
					}
				}else{
					alert("单笔投资大于等于不能为空");
					numMinInvestAmt.focus();
					return false;
				}
				if(numExtraInterestRate.value!=""){
					if(!reg.test(numExtraInterestRate.value)){
						alert("额外利率请输入正数或小数点后保留两位");
						return false;
					}
				}else{
					alert("额外利率不能为空");
					numExtraInterestRate.focus();
					return false;
				}
				var inputForm_subject = document.getElementById("inputForm_interestconfig");
				inputForm_interestconfig.submit();
			}
 </script>
