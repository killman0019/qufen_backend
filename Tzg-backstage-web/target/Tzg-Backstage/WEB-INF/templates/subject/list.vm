<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
		    function submitcfm(id) {
		        var flag = confirm("确认要提交该标的吗？");
		        if (flag == true){
		        	window.location.href="${www}/subject/submitApproval?id=" + id;
		        }
		    }
			
			 function deleteSubject(id) {
		        var flag = confirm("确认要删除该标的吗？");
		        if (flag == true){
		        	window.location.href="${www}/subject/deleteSubject?id=" + id;
		        }
		    }
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li class="active">标的列表</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>
	<div class="page-content">
		<div class="page-header">
		    <h1>标的列表</h1>
		</div>
		
		#if(${msg})
	     <div>
		     #if(${success})
	    		<li class="text-success">$!{msg}</li>
			 #else
	    		<li class="text-danger">$!{msg}</li>
			 #end    	
		 </div>
	    #end
		
		<div class="container-fluid">
		    <div class="dataTables_wrapper">
		        <div class="row">
		            <div class="col-sm-11">
		                <form class="form-inline" role="form">
		                    <div class="form-group">
		                        <label>标的名称：</label>
		                        <input type="text" name="vcName" value="$!{query.vcName}">
		                    </div>
							<div class="form-group">
		                        <label> 标的类型：</label>
		                        <select name="itype">
	                        		<option value="">全部</option>
		                            <option value="1" #if($query.itype ==1) selected #end>普通标</option>
		                            <option value="2" #if($query.itype ==2) selected #end>新手标</option>
									<option value="3" #if($query.itype && $query.itype ==3) selected #end>普通活动标</option>
									<option value="4" #if($query.itype && $query.itype ==4) selected #end>新手活动标</option>
									<option value="5" #if($query.itype && $query.itype ==5) selected #end>秒杀标</option>
									<option value="6" #if($query.itype && $query.itype ==6) selected #end>爆款标</option>
									<option value="7" #if($query.itype && $query.itype ==7) selected #end>多财标</option>
		                        </select>
		                    </div>		                    
		                    <div class="form-group">
		                        <label> 标的状态：</label>
		                        <select name="istate">
	                        		<option value="">全部</option>
		                            <option value="1" #if($query.istate =='1') selected #end>新录入</option>
		                            <option value="2" #if($query.istate =='2') selected #end>待审核</option>
		                            <option value="3" #if($query.istate =='3') selected #end>退回</option>
		                            <option value="4" #if($query.istate =='4') selected #end>作废</option>
		                            <option value="5" #if($query.istate =='5') selected #end>审核通过</option>
		                            <option value="6" #if($query.istate =='6') selected #end>待预告</option>
		                            <option value="10" #if($query.istate =='10') selected #end>待开标</option>
		                            <option value="11" #if($query.istate =='11') selected #end>投标中</option>
		                            <option value="12" #if($query.istate =='12') selected #end>还款中</option>
		                            <option value="13" #if($query.istate =='13') selected #end>结束</option>
		                            <option value="14" #if($query.istate =='14') selected #end>等待划款</option>
		                        </select>
		                    </div>
		                    <div class="form-group">
		                        <button class="btn btn-xs btn-primary" type="submit">查询</button>
		                    </div>
		                </form>
		            </div>
		        </div>
		        <table class="table table-striped table-bordered table-hover dataTable">
		            <thead>
		            <tr>
		          	    <th>标的类型</th>
		                <th>项目名称</th>
		                <th>标的名称</th>
		                <th>预计年利率</th>
		                <th>奖励年利率</th>
		                <th>融资金额</th>
		                <th>预告开始时间</th>
		                <th>募集开始时间</th>
		                <th>最终还款日期</th>
		                <th>标的状态 </th>
		                <th width="80px">操作</th>
		            </tr>
		            </thead>
		            <tbody>
		                #foreach($row in $result.rows)
		                <tr>
		                	<td>
		                		#if($row.itype && $row.itype ==1) 普通标
	                            #elseif($row.itype && $row.itype ==2) 新手标
								#elseif($row.itype && $row.itype ==3) 普通活动标
								#elseif($row.itype && $row.itype ==4) 新手活动标
								#elseif($row.itype && $row.itype ==5) 秒杀标
								#elseif($row.itype && $row.itype ==6) 爆款标
								#elseif($row.itype && $row.itype ==7) 多财标
	                            #end
		                	</td>
		                	<td> <a href="$!{www}/subject/getById?id=$!{row.id}">$!{row.projectName} </a> </td>
		                	<td> <a href="$!{www}/subject/getById?id=$!{row.id}">$!{row.vcName} </a></td>
		                	<td> $!{row.numInterestRate}% </td>
		                	<td> #if($row.numRewardRate&&$row.numRewardRate !='')  $!{row.numRewardRate}%
		                	#end
		                	</td>
		                	<td align="right"> $!{row.numTotalFinancing} </td>
		                	<td> $!{row.dtTrailerStr} </td>
		                	<td> $!{row.dtCollectStartStr} </td>
		                	<td> $!{row.dtRepaymentStr} </td>
		                	<td> 
		                	#if($row.istate =='1')新录入#end
		                    #if($row.istate =='2')待审核 #end
		                    #if($row.istate =='3')退回 #end
		                    #if($row.istate =='4')作废 #end
		                    #if($row.istate =='5')审核通过#end
		                    #if($row.istate =='6')待预告 #end
		                    #if($row.istate =='10')待开标#end
		                    #if($row.istate =='11')投标中 #end
		                    #if($row.istate =='12')还款中#end
		                    #if($row.istate =='13')结束 #end
		                    #if($row.istate =='14')等待划款 #end
		                	</td>
		                    <td>
		                    #if($!{row.istate}==1||$!{row.istate}==3)
		                    	<a href="$!{www}/subject/find?id=$!{row.id}">修改</a>|
		                    	<a onclick="submitcfm($!{row.id})" href="#">提交</a>
							#end
							#if($!{row.istate}==1)
								<a onclick="deleteSubject($!{row.id})" href="#">删除</a>
							#end
		                    #if($!{row.istate}==2)
		                    	#foreach($audit in $auditList)
		                    		#if($!{row.vcCurrentFlow}==$audit)
		                    			<a href="$!{www}/subject/toApproval?id=$!{row.id}">审核</a>
		                    		#end
		                    	#end
		                    #end
		                    </td>
		                </tr>
		                #end
		            </tbody>
		        </table>
		        <div class="row">
		            <div class="col-sm-11">
			        	<div class="red">$!{msg}</div>
			        </div>
			    </div>	
		        <div class="row">
		            <div class="col-sm-12">
		                <div class="dataTables_paginate paging_bootstrap">
		                    <ul class="pagination">
		                        #set($pageRequestURL="$!{www}/subject?pageIndex=[PAGINATION_PAGE_INDEX]#if($!{result.queryParameters})&$!{result.queryParameters}#end")
		                        #paginationHelper($result $pageRequestURL)
		                    </ul>
		                </div>
		            </div>
		       </div>
		       
		    </div>
		</div>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->
#if($request.getParameter("msg"))
	<script type="text/javascript">
		$(function(){
			alert('$!{request.getParameter("msg")}');
		})
	</script>
#end