<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li>
                <a href="${www}/messagemanage">站内信列表</a>
            </li>
            <li class="active">站内信详情</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>

    <div class="page-content">

        <div class="page-header">
            <h1>站内信详情</h1>
        </div>
        
        #if(${msg})
        	<div>
			#if(${success})
        		<li class="text-success">$!{msg}</li>
   			 #else
        		<li class="text-danger">$!{msg}</li>
   			 #end    	
   			</div>
   		#end	        
        
        <form class="form-horizontal">
             <div class="row">
                <div class="col-xs-12 col-md-8">
                       <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">类型 ：</label>
                        <div class="col-md-8 col-sm-8">
                                #if($param.imode ==1) 系统通知
                                #elseif($param.imode ==2) 活动公告
                                #elseif($param.imode ==3) 客户端公告-注意：此类内容请控制住100字以内且不要添加样式，只能编辑文字加换行
                                #end
                        </div>
                    </div>                 
                     <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">接收对象 ：</label>
                        <div class="col-md-8 col-sm-8">
                               #if($param.itype ==1)所有人
                               #elseif($param.itype ==2) 投资人
                               #elseif($param.itype ==3) 借款人
                               #end
                       </div>
                    </div>
                     <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">注册时间：</label>
                        <div class="col-md-8 col-sm-8">
                             $!{param.dtRegisterStartStr} - $!{param.dtRegisterEndStr}
                        </div>
                    </div>
                   <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">最后登录时间：</label>
                        <div class="col-md-8 col-sm-8">
                           $!{param.dtLastLoginStartStr}
								-
						   $!{param.dtLastLoginEndStr}
                        </div>
                    </div>
                   <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">最后充值时间：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.dtLastRechargeStartStr}
								-
						    $!{param.dtLastRechargeEndStr}
                        </div>
                    </div>
                   <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">最后投资时间：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.dtLastInvestStartStr}
								-
						    $!{param.dtLastInvestEndStr}
                        </div>
                    </div>
                   <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">累计充值金额：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.minTotalRecharge}
								-
						     $!{param.maxTotalRecharge}
                        </div>
                    </div>
                   <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">累计投资金额：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.minTotalInvest}
								-
						    $!{param.maxTotalInvest}
                        </div>
                    </div>
                   <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">账户余额：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.minTotalBalance}
								-
						    $!{param.maxTotalBalance}
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">最后一笔回款时间：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.dtLastRepayStartStr}
								-
						    $!{param.dtLastRepayEndStr}
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">账户资产总计：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.minTotalAsset}
								-
						    $!{param.maxTotalAsset}
                        </div>
                    </div>
                   <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">发送时间：</label>
                        <div class="col-md-8 col-sm-8">
                               #if($!{param.sType} == 1)
                                                                                                立即
                               #elseif($!{param.sType} == 2)
                                                                                                定时 $!{param.dtSendStr}
                               #end
                        </div>
                    </div>                               
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">标题：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.vcTitle}
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">副标题：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{param.vcSubTitle}
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">内容：</label>
                          <div class="col-md-8 col-sm-8">
                            $!{param.vcContent}
                        </div>                      
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">创建时间：</label>
                          <div class="col-md-8 col-sm-8">
                            $!{param.dtCreateStr}
                        </div>                      
                    </div> 
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">状态：</label>
                          <div class="col-md-8 col-sm-8">
		                	     #if($param.istate ==1) 刚录入
                                 #elseif($param.istate ==2) 发布中
                                 #elseif($param.istate ==3) 已发布
                                 #end		
                        </div>                      
                    </div>                              
                     <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">发布时间：</label>
                          <div class="col-md-8 col-sm-8">
                            $!{param.dtPublishStr}
                        </div>                      
                    </div>                                  
               </div>     
            </div>  
            <div class="clearfix form-actions">
                <div class="col-sm-offset-2 col-md-9">
		            <button class="btn btn-info" type="button" onclick="javascript:history.go(-1);">
		                <i class="icon-arrow-left bigger-110"></i>返回
		            </button>
                </div>
            </div>
    </div>
</div>

