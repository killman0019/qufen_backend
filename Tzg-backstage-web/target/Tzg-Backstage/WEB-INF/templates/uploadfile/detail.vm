<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}           
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li>
                <a href="${www}/uploadfile">上传资料列表</a>
            </li>
            <li class="active">上传资料详情</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>

    <div class="page-content">
        <div class="page-header">
            <h1>上传资料详情</h1>
        </div>
        
       #if(${msg})
        	<div>
			#if(${success})
        		<li class="text-success">$!{msg}</li>
   			 #else
        		<li class="text-danger">$!{msg}</li>
   			 #end    	
   			</div>
   		#end	        
         <form id="inputForm"  class="form-horizontal">       
        	<div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">类型：</label>
                        <div class="col-md-8 col-sm-8">
                            #if($!{uploadfile.itype} ==1)
                            	合同模板
                            #elseif($!{uploadfile.itype} ==2)
                            	风控附件文件（PC适用）
                            #elseif($!{uploadfile.itype} ==3)
                            	焦点图
                            #elseif($!{uploadfile.itype} ==4)
                             	保障措施图片
                            #elseif($!{uploadfile.itype} ==5)
                            	风控附件（非PC适用）
                        	#elseif($!{uploadfile.itype} ==6)
                        		风控审核清单
                        	#elseif($!{uploadfile.itype} ==7)
                        		合作机构LOGO
                        	#elseif($!{uploadfile.itype} ==8)
                        		媒体报告LOGO         
                        	#elseif($!{uploadfile.itype} ==9)
                        		新闻动态副图                 
                        	#elseif($!{uploadfile.itype} ==13)
                        		原理图    
                        	#elseif($!{uploadfile.itype} ==14)
                        		App欢迎页                                		          		               		                        		
                            #end
                        </div>
                    </div>
                </div>
             </div>
        	<div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">名称：</label>
                        <div class="col-md-8 col-sm-8">
                        	$!{uploadfile.vcName}
                        </div>
                    </div>
                </div>
             </div>
             <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">原始文件名称：</label>
                        <div class="col-md-8 col-sm-8">
                        	$!{uploadfile.vcOrnName}
                        </div>
                    </div>
                </div>
             </div>
             <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">服务器存放路径：</label>
                        <div class="col-md-8 col-sm-8">
                        	$!{uploadfile.vcUrl}
                        </div>
                    </div>
                </div>
             </div>
            <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-1 col-sm-1 control-label"></label>
                        <div class="col-md-8 col-sm-8">
                        <img src="$!{staticUploadUrl}$!{uploadfile.vcUrl}"  />
                        </div>
                    </div>
                </div>
             </div>
             <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">状态：</label>
                        <div class="col-md-8 col-sm-8">
                        	#if(${uploadfile.istate} == "1")
                        			未同步
                    		#elseif(${uploadfile.istate} == "2")
                    			已同步
                    		#end
                        </div>
                    </div>
                </div>
             </div>
             <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">创建时间：</label>
                        <div class="col-md-8 col-sm-8">
                        	#if($!{uploadfile.dtCreate})
                            		$dateTool.format("yyyy-MM-dd HH:mm:ss", $!{uploadfile.dtCreate}) 
                            #end
                        </div>
                    </div>
                </div>
             </div> 
            <div class="clearfix form-actions">
                <div class="row">
                    <div class="col-md-8">
                        <div class="col-sm-offset-4 col-md-9">
                            <button class="btn" type="button" onclick="javascript:history.go(-1);">
                                <i class="icon-arrow-left bigger-110"></i>返回
                            </button>
                        </div>
                    </div>
                </div>
            </div>
       </form>     
     </div>           
</div><!-- /.page-content -->
</div><!-- /.main-content -->
