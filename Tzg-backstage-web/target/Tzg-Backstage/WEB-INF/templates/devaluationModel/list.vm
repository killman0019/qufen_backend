<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li class="active">DevaluationModel列表</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>
	<div class="page-content">
		<div class="page-header">
		    <h1>DevaluationModel列表</h1>
		</div>
		<div class="container-fluid">
		    <div class="dataTables_wrapper">
		        <div class="row">
		            <div class="col-sm-11">
		                <form class="form-inline" role="form">
		                    <div class="form-group">
		                        <label>名称：</label>
		                        <input type="text" name="vcName" value="">
		                    </div>
		                    <div class="form-group">
		                        <button class="btn btn-xs btn-primary" type="submit">查询</button>
		                    </div>
		                </form>
		            </div>
                    <div class="col-sm-1">
                        <a href="$!{www}/devaluationModel/new" class="btn btn-success btn-xs">添加</a>
                    </div>
		        </div>
		        <table class="table table-striped table-bordered table-hover dataTable">
		            <thead>
		            <tr>
		                <th>modelId</th>
		                <th>描述</th>
		                <th>描述</th>
		                <th>占比，百分比2位数</th>
		                <th>createTime</th>
		                <th>updateTime</th>
		                <th>createUserId</th>
		                <th>status</th>
		                <th>评测类型：1-简单测评；2-系统定义专业测评;3-用户自定义专业测评</th>
		                <th>批次号</th>
		             
		                <th>操作</th>
		               
		            </tr>
		            </thead>
		            <tbody>
		                #foreach($row in $result.rows)
		                <tr>
		                	<td> $!{row.modelId} </td>
		                	<td> $!{row.modelName} </td>
		                	<td> $!{row.modelDesc} </td>
		                	<td> $!{row.modelWeight} </td>
		                	<td> $!{row.createTime} </td>
		                	<td> $!{row.updateTime} </td>
		                	<td> $!{row.createUserId} </td>
		                	<td> $!{row.status} </td>
		                	<td> $!{row.modelType} </td>
		                	<td> $!{row.batchId} </td>
		                    <td><a href="$!{www}/devaluationModel/find?id=$!{row.id}">修改</a>   ｜
		                        <a onclick="delcfm()" href="$!{www}/devaluationModel/delete?id=$!{row.id}">删除</a>
		                    </td>
		                </tr>
		                #end
		            </tbody>
		        </table>
		        <div class="row">
		            <div class="col-sm-12">
		                <div class="dataTables_paginate paging_bootstrap">
		                    <ul class="pagination">
		                    	#set($pageRequestURL="$!{www}/devaluationModel?pageIndex=[PAGINATION_PAGE_INDEX]#if($!{result.queryParameters})&$!{result.queryParameters}#end")
		                    	#paginationHelper($result $pageRequestURL)
		                    </ul>
		                </div>
		            </div>
		       </div>
		       
		    </div>
		</div>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->