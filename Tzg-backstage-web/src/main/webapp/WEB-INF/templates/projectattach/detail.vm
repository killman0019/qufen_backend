<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
    	<script src="${www}/js/my97DatePicker/WdatePicker.js"></script>
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
            
            
            function submitFun(){
				//...            
            	var inputForm = document.getElementById("inputForm");
            	inputForm.submit();
            }
            
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li class="active">风控附件信息详情</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>

    <div class="page-content">
        <div class="page-header">
            <h1>风控附件信息详情</h1>
        </div>
        
        #if(${msg})
			<div>
				 #if(${success})
		    		<li class="text-success">$!{msg}</li>
				 #else
		    		<li class="text-danger">$!{msg}</li>
				 #end    	
			</div>
	     #end
        
    		<div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">项目：</label>
                        <div class="col-md-8 col-sm-8">
                            $!{iProjectName}
                        </div>
                    </div>
                </div>
             </div>
             <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">风控附件名称：</label>
                        <div class="col-md-8 col-sm-8">
                        	$!{projectattach.vcAttachName}
                        </div>
                    </div>
                </div>
             </div>
             <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">文件路径：</label>
                        <div class="col-md-8 col-sm-8">
                        	$!{projectattach.vcFilePath}
                        </div>
                    </div>
                </div>
             </div>
             
             
             
             <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">附件内容：</label>
                        <div class="col-md-8 col-sm-8">
                        	<a href="${www}/$!{filePath}">附件下载</a>
                        </div>
                    </div>
                </div>
             </div>
             <div class="row">
                <div class="col-xs-12 col-md-8">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">排列顺序：</label>
                        <div class="col-md-8 col-sm-8">
                        	$!{projectattach.iSort}
                        </div>
                    </div>
                </div>
             </div> 
    </div>

            <div class="clearfix form-actions">
                <div class="row">
                    <div class="col-md-8">
                        <div class="col-sm-offset-4 col-md-9">
                            <button class="btn" type="button" onclick="javascript:history.go(-1);">
                                <i class="icon-arrow-left bigger-110"></i>返回
                            </button>
                        </div>
                    </div>
                </div>
            </div>
</div><!-- /.page-content -->
</div><!-- /.main-content -->
