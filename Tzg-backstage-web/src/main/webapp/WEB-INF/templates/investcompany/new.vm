<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li>
                <a href="${www}/investcompany">Investcompany列表</a>
            </li>
            <li class="active">新增Investcompany</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>

    <div class="page-content">
        <div class="page-header">
            <h1>新增Investcompany</h1>
        </div>
        <form action="${www}/investcompany/save" method="post" class="form-horizontal">
            <div class="row">
                <div class="col-xs-12 col-md-4">
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">id：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="id" value="$!{param.id}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">iloginAccountId：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="iloginAccountId" value="$!{param.iloginAccountId}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">numAvailable：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="numAvailable" value="$!{param.numAvailable}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">numLaunchTotal：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="numLaunchTotal" value="$!{param.numLaunchTotal}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">numRetrieveTotal：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="numRetrieveTotal" value="$!{param.numRetrieveTotal}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">numTotalServiceTotalFee：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="numTotalServiceTotalFee" value="$!{param.numTotalServiceTotalFee}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">numTotalExpenseTotalFee：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="numTotalExpenseTotalFee" value="$!{param.numTotalExpenseTotalFee}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">1 -- 有效；             2 -- 冻结中；             3 -- 无效。：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="istatus" value="$!{param.istatus}"/>
                        </div>
                    </div>
                    <div class="red">$!{msg}</div>
               	</div>
            </div>
            <div class="clearfix form-actions">
                <div class="col-sm-offset-2 col-md-9">
                    <button class="btn btn-info" type="submit">
                        <i class="icon-ok bigger-110"></i> 提交
                    </button>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		            <button class="btn btn-info" type="button" onclick="javascript:history.go(-1);">
		                <i class="icon-arrow-left bigger-110"></i>返回
		            </button>
                </div>
            </div>
</form>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->