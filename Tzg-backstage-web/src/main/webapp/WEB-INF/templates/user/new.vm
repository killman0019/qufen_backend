<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="${www}/main">主页</a>
            </li>

            <li>
                <a href="${www}/user">用户信息列表</a>
            </li>
            <li class="active">新增用户</li>
        </ul><!-- .breadcrumb -->

        <div class="nav-search" id="nav-search">
            <form class="form-search">
						<span class="input-icon">
							<input type="text" placeholder="Search ..." class="nav-search-input" id="nav-search-input" autocomplete="off" />
							<i class="icon-search nav-search-icon"></i>
						</span>
            </form>
        </div><!-- #nav-search -->
    </div>

    <div class="page-content">
        <div class="page-header">
            <h1>新增用户</h1>
        </div>
        <form action="${www}/user/save" method="post" class="form-horizontal">
            <div class="row">
                <div class="col-xs-12 col-md-4">
                  
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">昵称：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="userName" value="$!{param.userName}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">性别:1男；2女：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="sex" value="$!{param.sex}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">头像：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="icon" value="$!{param.icon}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">手机号码：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="mobile" value="$!{param.mobile}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">邮箱：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="email" value="$!{param.email}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">微信号：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="wechat" value="$!{param.wechat}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">密码：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="password" value="$!{param.password}"/>
                        </div>
                    </div>
                 
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">用户类型:(1-普通用户；2-项目方；3-评测机构；4-机构用户)</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="userType" value="$!{param.userType}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">用户等级：(1-普通用户；2-高级用户;3-VIP)</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="userDegree" value="$!{param.userDegree}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">个性签名：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="userSignature" value="$!{param.userSignature}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">粉丝数量：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="fansNum" value="$!{param.fansNum}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">收获点赞数量：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="praiseNum" value="$!{param.praiseNum}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">发表评测数量：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="evaluationNum" value="$!{param.evaluationNum}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">发表讨论数量：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="discussNum" value="$!{param.discussNum}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">发表文章数量：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="articleNum" value="$!{param.articleNum}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">账号币值：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="kffCoinNum" value="$!{param.kffCoinNum}"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">所在地区域名称:</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="areaName" value="$!{param.areaName}"/>
                        </div>
                    </div>
        
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">备注信息：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="memo" value="$!{param.memo}"/>
                        </div>
                    </div>
  
                    <div class="form-group">
                        <label class="col-md-4 col-sm-2 control-label">推荐用户ID：</label>
                        <div class="col-md-8 col-sm-8">
                            <input type="text" class="col-sm-12" name="referUserId" value="$!{param.referUserId}"/>
                        </div>
                    </div>
  
                    <div class="red">$!{msg}</div>
               	</div>
            </div>
            <div class="clearfix form-actions">
                <div class="col-sm-offset-2 col-md-9">
                    <button class="btn btn-info" type="submit">
                        <i class="icon-ok bigger-110"></i> 提交
                    </button>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
		            <button class="btn btn-info" type="button" onclick="javascript:history.go(-1);">
		                <i class="icon-arrow-left bigger-110"></i>返回
		            </button>
                </div>
            </div>
</form>
    </div><!-- /.page-content -->
</div><!-- /.main-content -->